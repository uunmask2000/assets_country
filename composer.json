{
    "name": "uunmask2000_kk/assets_country",
    "authors": [
        {
            "name": "uunmask2000_kk",
            "email": "uunmask2000@gmail.com",
            "homepage": "https://twitter.com/uunmask2000",
            "role": "Developer"
        }
    ],
    "description": "根據一些國家(地區)資訊，返回對應的國家(地區)縮碼國旗* ",
    "type": "library",
    "license": "LGPL-3.0",
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/uunmask2000/assets_country/issues"
    },
    "require": {
        "php": ">=7.4",
        "pragmarx/countries": "0.8.2",
        "psr/simple-cache":"^1.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-zip": "*"
    },
    "suggest": {
        "ext-zip": "Needed to store or download multiple files at once."
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AssetsCountryService\\": "src"
        }
    },
    "extra": {
        "branch-alias": {}
    }
}